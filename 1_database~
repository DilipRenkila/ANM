CREATE TABLE IF NOT EXISTS DEVICES ( id int(11) NOT NULL AUTO_INCREMENT,IP tinytext NOT NULL,PORT int(11) NOT NULL,COMMUNITY tinytext NOT NULL,PRIMARY KEY (id))ENGINE=InnoDB DEFAULT CHARSET=latin1  AUTO_INCREMENT=1;



